{
    "$schema": "http://json-schema.org/draft-07/schema",
    "description": "用于静态分析的通用数据流规则定义",
    "title": "The common dataflow rules schema for static analysis 1.0",
    "type": "object",
    "properties": {
        "RulePackage": {
            "description": "规则集信息",
            "title": "Rule package",
            "$ref": "#/$defs/RulePackage"
        },
        "Rules": {
            "description": "规则表",
            "title": "Rules",
            "$ref": "#/$defs/Rules"
        },
        "RuleMessages": {
            "description": "规则消息表",
            "title": "Rule messages",
            "$ref": "#/$defs/RuleMessages"
        }
    },
    "additionalProperties": false,
    "required": [
        "RulePackage",
        "Rules",
        "RuleMessages"
    ],
    "examples": [
        {
            "RulePackage": {
                "Name": "RulePackage",
                "Description": "Description",
                "Version": "Version",
                "Language": "JAVA",
                "Locale": "CN"
            },
            "Rules": {
                "Note": "note",
                "TaintSourceRule": []
            },
            "RuleMessages": []
        }
    ],
    "$defs": {
        "RulePackage": {
            "$anchor": "RulePackage",
            "description": "规则集基本信息定义",
            "title": "Rule package definition",
            "type": "object",
            "required": [
                "Name",
                "Description",
                "Version",
                "Language",
                "Locale"
            ],
            "properties": {
                "Name": {
                    "description": "规则集的名称",
                    "title": "Rule package name",
                    "type": "string",
                    "minLength": 1,
                    "examples": [
                        "Dataflow Rules Package"
                    ]
                },
                "Description": {
                    "description": "规则集的描述",
                    "title": "Rule package description",
                    "type": "string",
                    "minLength": 1,
                    "examples": [
                        "Dataflow Rule Package description"
                    ]
                },
                "Version": {
                    "description": "规则集的版本",
                    "title": "Rule package version",
                    "type": "string",
                    "minLength": 1,
                    "examples": [
                        "ver 2021.11.09"
                    ]
                },
                "Language": {
                    "description": "规则集适用开发语言",
                    "title": "Rule package develop language",
                    "$ref": "#/$defs/Language"
                },
                "Locale": {
                    "description": "规则信息显示语言",
                    "title": "Message locale",
                    "$ref": "#/$defs/Locale"
                }
            },
            "additionalProperties": false
        },
        "Rules": {
            "$anchor": "Rules",
            "description": "规则表定义",
            "title": "Rules definition",
            "type": "object",
            "required": [
                "Note"
            ],
            "properties": {
                "Note": {
                    "description": "规则表描述",
                    "title": "Rules note",
                    "$ref": "#/$defs/Note"
                },
                "EntryPointRules": {
                    "description": "入口点规则列表",
                    "title": "Entry point rules",
                    "$ref": "#/$defs/EntryPointRule"
                },
                "TaintSourceRules": {
                    "description": "污染源规则列表",
                    "title": "Taint source rules",
                    "$ref": "#/$defs/TaintSourceRule"
                },
                "TaintPassthroughRules": {
                    "description": "污点传递规则列表",
                    "title": "Taint passthrough rules",
                    "$ref": "#/$defs/TaintPassthroughRule"
                },
                "TaintSinkRules": {
                    "description": "污染爆发点规则列表",
                    "title": "Taint sink rules",
                    "$ref": "#/$defs/TaintSinkRule"
                },
                "TaintCleanseRules": {
                    "description": "污点清理规则列表",
                    "title": "Taint cleanse rules",
                    "$ref": "#/$defs/TaintCleanseRule"
                }
            },
            "additionalProperties": false
        },
        "EntryPointRule": {
            "$anchor": "EntryPointRule",
            "description": "入口点规则列表定义",
            "title": "Entry point rule definition",
            "type": "array",
            "items": {
                "description": "入口点规则列表成员定义",
                "title": "Entry point rule item definition",
                "type": "object",
                "minProperties": 3,
                "maxProperties": 3,
                "required": [
                    "Rule",
                    "Functions",
                    "EntryPoint"
                ],
                "properties": {
                    "Rule": {
                        "description": "入口点规则基本信息",
                        "title": "Entry point rule base info",
                        "$ref": "#/$defs/RuleBaseInfo"
                    },
                    "Functions": {
                        "description": "入口点规则函数列表",
                        "title": "Entry point rule functions",
                        "type": "array",
                        "minItems": 1,
                        "items": {
                            "$ref": "#/$defs/FunctionIdentifier"
                        }
                    },
                    "EntryPoint": {
                        "description": "入口点信息",
                        "title": "Entry point info",
                        "$ref": "#/$defs/EntryPoint"
                    }
                },
                "additionalProperties": false
            }
        },
        "TaintSourceRule": {
            "$anchor": "TaintSourceRule",
            "description": "污染源规则列表定义",
            "title": "Taint source rule definition",
            "type": "array",
            "items": {
                "title": "Taint source rule item definition",
                "description": "污染源规则列表成员定义",
                "type": "object",
                "minProperties": 3,
                "maxProperties": 3,
                "required": [
                    "Rule",
                    "Functions",
                    "Source"
                ],
                "properties": {
                    "Rule": {
                        "description": "污染源规则基本信息",
                        "title": "Taint source rule base info",
                        "$ref": "#/$defs/RuleBaseInfo"
                    },
                    "Functions": {
                        "description": "污染源规则函数列表",
                        "title": "Taint source rule functions",
                        "type": "array",
                        "minItems": 1,
                        "items": {
                            "$ref": "#/$defs/FunctionIdentifier"
                        }
                    },
                    "Source": {
                        "description": "污染源信息",
                        "title": "Taint source info",
                        "$ref": "#/$defs/Source"
                    }
                },
                "additionalProperties": false
            }
        },
        "TaintPassthroughRule": {
            "$anchor": "TaintPassthroughRule",
            "description": "污点传递规则列表定义",
            "title": "Taint passthrough rule definition",
            "type": "array",
            "items": {
                "description": "污点传递规则列表成员定义",
                "title": "Taint passthrough rule item definition",
                "type": "object",
                "minProperties": 3,
                "maxProperties": 3,
                "required": [
                    "Rule",
                    "Functions",
                    "Passthrough"
                ],
                "properties": {
                    "Rule": {
                        "description": "污染传递规则基本信息",
                        "title": "Taint passthrough rule base info",
                        "$ref": "#/$defs/RuleBaseInfo"
                    },
                    "Functions": {
                        "description": "污染传递规则函数列表",
                        "title": "Taint passthrough rule functions",
                        "type": "array",
                        "minItems": 1,
                        "items": {
                            "$ref": "#/$defs/FunctionIdentifier"
                        }
                    },
                    "Passthrough": {
                        "description": "污染传递信息",
                        "title": "Taint passthrough info",
                        "$ref": "#/$defs/Passthrough"
                    }
                },
                "additionalProperties": false
            }
        },
        "TaintSinkRule": {
            "$anchor": "TaintSinkRule",
            "description": "污染爆发点规则列表定义",
            "title": "Taint sink rule definition",
            "type": "array",
            "examples": [
                {
                    "Rule": {
                        "RuleId": "74420652-95a6-4c52-a51e-7f4358f09350",
                        "RuleName": "checker"
                    },
                    "Vulnerability": {
                        "Category": "Category",
                        "Subcategory": "Subcategory",
                        "IssueType": "IssueType",
                        "CWE": "95",
                        "Severity": "Critical"
                    },
                    "Functions": [
                        {
                            "Class": {
                                "Value": "value"
                            }
                        }
                    ],
                    "Sink": {
                        "InArgs": "1",
                        "ReportMsgId": "msgid"
                    }
                }
            ],
            "items": {
                "description": "污染爆发点规则列表成员定义",
                "title": "Taint sink rule item definition",
                "type": "object",
                "minProperties": 4,
                "maxProperties": 4,
                "required": [
                    "Rule",
                    "Vulnerability",
                    "Functions",
                    "Sinks"
                ],
                "properties": {
                    "Rule": {
                        "description": "污染爆发点规则基本信息",
                        "title": "Taint sink rule base info",
                        "$ref": "#/$defs/CheckerInfo"
                    },
                    "Vulnerability": {
                        "description": "缺陷信息",
                        "title": "Vulnerability info",
                        "$ref": "#/$defs/Vulnerability"
                    },
                    "Functions": {
                        "description": "污染爆发点规则函数列表",
                        "title": "Taint sink rule functions",
                        "type": "array",
                        "minItems": 1,
                        "items": {
                            "$ref": "#/$defs/FunctionIdentifier"
                        }
                    },
                    "Sinks": {
                        "description": "污染爆发信息",
                        "title": "Sink info",
                        "type": "array",
                        "items": {
                            "$ref": "#/$defs/Sink"
                        }
                    }
                },
                "additionalProperties": false
            }
        },
        "TaintCleanseRule": {
            "$anchor": "TaintCleanseRule",
            "description": "污点清理规则列表定义",
            "title": "Taint cleanse rule definition",
            "type": "array",
            "items": {
                "description": "污点清理规则列表成员定义",
                "title": "Taint cleanse rule item definition",
                "type": "object",
                "minProperties": 3,
                "maxProperties": 3,
                "required": [
                    "Rule",
                    "Functions",
                    "Cleanse"
                ],
                "properties": {
                    "Rule": {
                        "description": "污染清理规则基本信息",
                        "title": "Taint cleanse rule base info",
                        "$ref": "#/$defs/RuleBaseInfo"
                    },
                    "Functions": {
                        "description": "污染清理规则函数列表",
                        "title": "Taint cleanse rule functions",
                        "type": "array",
                        "minItems": 1,
                        "items": {
                            "$ref": "#/$defs/FunctionIdentifier"
                        }
                    },
                    "Cleanse": {
                        "description": "污染清理信息",
                        "title": "Cleanse info",
                        "$ref": "#/$defs/Cleanse"
                    }
                },
                "additionalProperties": false
            }
        },
        "RuleBaseInfo": {
            "$anchor": "RuleBaseInfo",
            "description": "规则基本信息定义",
            "title": "Rule base info definition",
            "type": "object",
            "required": [
                "RuleId"
            ],
            "properties": {
                "RuleId": {
                    "description": "规则id, 每个规则需要有一个全局的唯一标识,可用UUID生成",
                    "title": "Rule id, should be uniquite",
                    "$ref": "#/$defs/RuleId"
                },
                "Note": {
                    "description": "规则基本信息描述",
                    "title": "Rule base info description",
                    "$ref": "#/$defs/Note"
                },
                "RefInfos": {
                    "description": "规则参考信息",
                    "title": "Rule reference info",
                    "$ref": "#/$defs/RefInfo"
                }
            },
            "additionalProperties": false
        },
        "CheckerInfo": {
            "$anchor": "CheckerInfo",
            "description": "检查规则信息定义",
            "title": "Rule checker info definition",
            "type": "object",
            "required": [
                "RuleId",
                "RuleName"
            ],
            "properties": {
                "RuleId": {
                    "description": "检查规则id, 每一个检查规则需要有一个全局的唯一标识,可用UUID生成",
                    "title": "Checker id, should be uniquite",
                    "$ref": "#/$defs/RuleId"
                },
                "RuleName": {
                    "description": "检查规则名",
                    "title": "Checker name",
                    "type": "string",
                    "minLength": 1,
                    "examples": [
                        "Code_Injection"
                    ]
                },
                "Note": {
                    "description": "检查规则描述",
                    "title": "Checker description",
                    "$ref": "#/$defs/Note"
                },
                "RefInfos": {
                    "description": "检查规则参考信息",
                    "title": "Checker reference info",
                    "$ref": "#/$defs/RefInfo"
                }
            },
            "additionalProperties": false
        },
        "Vulnerability": {
            "$anchor": "Vulnerability",
            "description": "缺陷信息定义",
            "title": "Vulnerability info definition",
            "type": "object",
            "examples": [
                {
                    "Category": "Injection",
                    "Subcategory": "Code Injection",
                    "IssueType": "Untrusted Input",
                    "CWE": "95",
                    "Severity": "Critical"
                }
            ],
            "required": [
                "Category",
                "Subcategory",
                "IssueType",
                "CWE",
                "Severity"
            ],
            "properties": {
                "Category": {
                    "description": "缺陷分类",
                    "title": "Vulnerability Category",
                    "type": "string",
                    "minLength": 1,
                    "examples": [
                        "Injection"
                    ]
                },
                "Subcategory": {
                    "description": "缺陷子类",
                    "title": "Vulnerability Subcategory",
                    "type": "string",
                    "minLength": 1,
                    "examples": [
                        "Code Injection"
                    ]
                },
                "IssueType": {
                    "description": "缺陷场景",
                    "title": "Vulnerability issue type",
                    "type": "string",
                    "examples": [
                        "Untrusted Input"
                    ]
                },
                "CWE": {
                    "description": "CWE",
                    "title": "CWE",
                    "$ref": "#/$defs/CWE"
                },
                "Severity": {
                    "description": "缺陷危害等级",
                    "title": "Vulnerability severity",
                    "$ref": "#/$defs/Severity"
                }
            },
            "additionalProperties": false
        },
        "RuleMessages": {
            "$anchor": "RuleMessages",
            "description": "规则的信息列表定义",
            "title": "Rule messages definition",
            "type": "array",
            "items": {
                "description": "规则的信息列表成员定义",
                "title": "Rule messages item definition",
                "type": "object",
                "minProperties": 2,
                "maxProperties": 2,
                "examples": [
                    {
                        "Id": "Code.Injection.exec|eval",
                        "Msg": "Untrusted data pass to eval 1st parameter will cause Code Injection."
                    }
                ],
                "required": [
                    "Id",
                    "Msg"
                ],
                "properties": {
                    "Id": {
                        "description": "规则信息ID",
                        "title": "Rule messages Id",
                        "type": "string",
                        "examples": [
                            "Code.Injection.exec|eval"
                        ]
                    },
                    "Msg": {
                        "description": "规则信息",
                        "title": "Rule messages",
                        "type": "string",
                        "examples": [
                            "Untrusted data pass to eval 1st parameter will cause Code Injection."
                        ]
                    }
                },
                "additionalProperties": false
            }
        },
        "Language": {
            "$anchor": "Language",
            "description": "规则集适用开发语言定义",
            "title": "Rule package develop language definition",
            "enum": [
                "C",
                "CPP",
                "C/CPP",
                "JAVA",
                "PYTHON",
                "GO",
                "JAVASCRIPT"
            ]
        },
        "Locale": {
            "$anchor": "Locale",
            "description": "规则信息显示语言定义",
            "title": "Message locale definition",
            "enum": [
                "EN",
                "CN"
            ]
        },
        "ValueType": {
            "$anchor": "ValueType",
            "description": "值的类型定义",
            "title": "Value type definition",
            "type": "object",
            "minProperties": 1,
            "maxProperties": 1,
            "properties": {
                "Value": {
                    "description": "值的匹配",
                    "title": "Value matching",
                    "type": "string",
                    "examples": [
                        "Python Dataflow Rules"
                    ]
                },
                "Pattern": {
                    "description": "正则表示式匹配",
                    "title": "Pattern matching",
                    "type": "string",
                    "format": "regex",
                    "examples": [
                        "Python Dataflow Rule Package"
                    ]
                }
            },
            "additionalProperties": false
        },
        "Severity": {
            "$anchor": "Severity",
            "description": "缺陷的危害等级定义",
            "title": "Vulnerability severity definition",
            "enum": [
                "Fatal",
                "Critical",
                "Medium",
                "Normal",
                "Suggest"
            ]
        },
        "CWE": {
            "$anchor": "CWE",
            "description": "CWE定义",
            "title": "CWE definition",
            "type": "string",
            "pattern": ".{0}|^[1-9][0-9]{0,3}(,[1-9][0-9]{0,3})*$"
        },
        "Note": {
            "$anchor": "Note",
            "description": "描述或注释定义",
            "title": "Note definition",
            "type": "string"
        },
        "RuleId": {
            "$anchor": "RuleId",
            "description": "规则编号定义",
            "title": "Rule Id definition",
            "type": "string",
            "examples": [
                "74420652-95a6-4c52-a51e-7f4358f09350"
            ]
        },
        "RefInfo": {
            "$anchor": "RefInfo",
            "description": "参考信息列表定义",
            "title": "Reference info definition",
            "type": "array",
            "items": [
                {
                    "description": "参考信息列表成员定义",
                    "title": "Reference info item definition",
                    "type": "object",
                    "required": [
                        "Key",
                        "Value"
                    ],
                    "properties": {
                        "Key": {
                            "description": "参考信息关键值",
                            "title": "Reference key",
                            "type": "string",
                            "minLength": 1,
                            "examples": [
                                "packge"
                            ]
                        },
                        "Value": {
                            "description": "参考信息值",
                            "title": "Reference value",
                            "type": "string",
                            "examples": [
                                "Python Core"
                            ]
                        }
                    },
                    "additionalProperties": false
                }
            ]
        },
        "Parameters": {
            "$anchor": "Parameters",
            "description": "函数参数定义",
            "title": "Function parameters definition",
            "type": "object",
            "minProperties": 1,
            "properties": {
                "ParamTypes": {
                    "description": "函数参数成员定义",
                    "title": "Function parameters item definition",
                    "type": "array",
                    "minItems": 1,
                    "items": [
                        {
                            "properties": {
                                "ParamInd": {
                                    "description": "参数的位置",
                                    "title": "The index of paramter position",
                                    "type": "integer"
                                },
                                "ParamType": {
                                    "description": "参数的类型",
                                    "title": "Paramter type",
                                    "$ref": "#/$defs/ValueType"
                                }
                            }
                        }
                    ]
                },
                "varArg": {
                    "description": "函数是否有可变参数",
                    "title": "Function has variable parameter",
                    "type": "boolean",
                    "default": false,
                    "examples": [
                        false
                    ]
                }
            },
            "additionalProperties": false
        },
        "Modifier": {
            "$anchor": "Modifier",
            "description": "函数的修饰符定义",
            "title": "Function modifier definition",
            "enum": [
                "public",
                "private",
                "protected",
                "static",
                "final"
            ]
        },
        "FunctionIdentifier": {
            "$anchor": "FunctionIdentifier",
            "description": "函数定义",
            "title": "Function indentifier definition",
            "type": "object",
            "minProperties": 1,
            "properties": {
                "Namespace": {
                    "description": "命名空间",
                    "title": "The Namespace",
                    "$ref": "#/$defs/ValueType"
                },
                "Class": {
                    "description": "类名",
                    "title": "The Class",
                    "$ref": "#/$defs/ValueType"
                },
                "Function": {
                    "title": "The Function",
                    "description": "函数名",
                    "$ref": "#/$defs/ValueType"
                },
                "Parameters": {
                    "description": "函数参数",
                    "title": "The Parameters",
                    "$ref": "#/$defs/Parameters"
                },
                "ReturnType": {
                    "description": "函数返回的类型",
                    "title": "The ReturnType",
                    "type": "string"
                },
                "ApplyTo": {
                    "description": "函数的适配扩展到接口、实现, 以及所有重写的函数",
                    "title": "The adaptation of the function is extended to the interface, implementation, and all overridden functions",
                    "type": "boolean",
                    "default": true
                },
                "Modifier": {
                    "description": "函数修饰符",
                    "title": "Function modifier",
                    "$ref": "#/$defs/Modifier"
                },
                "Exclude": {
                    "description": "不包含的函数列表",
                    "title": "Exclude function list",
                    "type": "array",
                    "items": {
                        "$ref": "#/$defs/FunctionIdentifier"
                    }
                },
                "additionalProperties": false
            }
        },
        "Arguments": {
            "$anchor": "Arguments",
            "description": "函数参数值的多个定义。使用逗号分隔列表。接受以下形式的表达式:x- 单个参数,其中 x 是一个数字、this、return或globals; 当 x 是一个数字时,它指示函数的第 x 个参数,从 0 开始; x.member - 给定参数的单个成员字段; n...- 参数 n 之后的所有参数,包括参数 n; n1-n2 - n1 和 n2 之间的所有参数",
            "title": "Arguments definition",
            "type": "string",
            "pattern": "([0-9]+|[0-9]+\\.\\.\\.|[0-9]+-[0-9]+|this|return|globals)(\\.[\\$a-zA-Z0-9_\\-]+)*(,([0-9]+|[0-9]+\\.\\.\\.|[0-9]+-[0-9]+|this|return|globals)(\\.[\\$a-zA-Z0-9_\\-]+)*)*",
            "minLength": 1,
            "examples": [
                "0"
            ]
        },
        "Argument": {
            "$anchor": "Argument",
            "type": "string",
            "pattern": "[0-9]+|this|return",
            "title": "The Argument",
            "description": "函数的一个参数。值允许this,return;或使用数字n,能代表参数的位置坐标,位置坐标从0开始。",
            "minLength": 1,
            "examples": [
                "0"
            ]
        },
        "ArgumentCompare": {
            "$anchor": "ArgumentCompare",
            "type": "object",
            "title": "The ArgumentCompare",
            "description": "参数比较:ArgumentCompare",
            "required": [
                "Argument",
                "Value"
            ],
            "properties": {
                "Argument": {
                    "$ref": "#/$defs/Argument"
                },
                "Value": {
                    "type": "string",
                    "description": "参数值"
                },
                "Mode": {
                    "type": "string",
                    "description": "参数值模式"
                }
            },
            "additionalProperties": false
        },
        "ReportMsgId": {
            "$anchor": "ReportMsgId",
            "type": "string",
            "title": "The ReportMsgId",
            "description": "规则的扩展参考信息ID",
            "minLength": 1,
